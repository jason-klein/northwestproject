<%= simple_form_for(@meeting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :topic %>
    <%= f.input :meeting_date %>
    <%= f.association :cohort %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
